{
  "source": "./src",
  "destination": "./esdoc",
  "index": "./DOC.md",
  "excludes": ["util\\.js", "converter\\.js"],
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "brand": {
          "title": "SimpleDom-component"
        }
      }
    },
    {
      "name": "esdoc-importpath-plugin",
      "option": {

        "replaces": [
          {
            "from": "^.*",
            "to": ""
          }
        ]
      }
    },

    {"name": "esdoc-ecmascript-proposal-plugin", "option": {"all": true}}
  ]
}
